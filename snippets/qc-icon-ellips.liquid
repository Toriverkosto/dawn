{%- if value == 'Passed' -%}
  {%- render 'icon-passed-ellips' -%}
{% elsif value == 'Not Passed' %}
  {%- render 'icon-not-passed-ellips' -%}
{% comment %}{% elsif value == 'Certified' %}{% endcomment %}
